{
  "name": "spotify-app",
  "displayName": "spotify-app",
  "expo": {
    "name": "Spotify",
    "slug": "spotify-app",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trebias.ios.spotify"
    },
    "android": {
      "package": "com.trebias.android.spotify",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
